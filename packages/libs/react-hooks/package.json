{
  "name": "@rb/react-hooks",
  "version": "0.0.1",
  "description": "Shared react hooks",
  "main": "dist/index",
  "types": "dist/index",
  "sideEffects": false,
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "dev": "yarn run watch",
    "docs": "yarn run watch",
    "docs:build": "yarn run build",
    "next-dev": "yarn run watch",
    "watch": "tsc -p tsconfig.build.json -w --preserveWatchOutput"
  },
  "devDependencies": {
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
