{
  "name": "@rb/react-components",
  "version": "0.0.1",
  "main": "dist/index",
  "types": "dist/index",
  "sideEffects": false,
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "dev": "yarn run watch",
    "docs": "yarn run watch",
    "next-dev": "yarn run watch",
    "show-config": "tsc --showConfig",
    "prepublishOnly": "yarn run build",
    "watch": "yarn compile & tsc -p tsconfig.build.json -w --preserveWatchOutput",
    "jest:unit": "jest --config jest.config.unit.js"
  },
  "dependencies": {
    "@primer/styled-octicons": "^13.0.0",
    "@rb/react-style-system": "*",
    "@react-icons/all-files": "^4.1.0",
    "humps": "^2.0.1",
    "mdx-observable": "^0.2.0",
    "prismjs": "^1.23.0",
    "prism-react-renderer": "^1.2.1",
    "react-element-to-jsx-string": "^14.3.2",
    "react-is": "^17.0.2",
    "react-select": "^4.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/styled-system": "^5.1.11",
    "@types/testing-library__jest-dom": "^5.9.5",
    "csstype": "^3.0.8",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "styled-components": "*",
    "styled-system": "*"
  }
}
