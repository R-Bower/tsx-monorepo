{
  "name": "@rb/react-components",
  "version": "0.0.1",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "dev": "npm run watch",
